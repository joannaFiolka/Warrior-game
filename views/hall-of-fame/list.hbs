<section class="hall-of-fames">
    <div class="list-hall-of-fames">
<h1> Sala sława</h1>
<table>
    <thead>
    <tr>
        <th>#</th>
        <th>Ilość zwycięstw</th>
        <th>Imię</th>
    </tr>
    </thead>
    <tbody>
     {{#each warriors}}
         <tr>
             <th>
                 {{this.place}}
             </th>
             <td>
                 {{this.warrior.wins}}
             </td>
             <td>
                 {{this.warrior.name}}
             </td>
         </tr>
     {{/each}}
    </tbody>
</table>
    </div>
</section>